{
    "name": "temidaio/tall-components",
    "description": "It is an example template for Laravel packages. Fill or change it the way you like.",
    "keywords": [
        "laravel blade components",
        "tall components",
        "tall stack components",
        "blade components"
    ],
    "homepage": "https://github.com/temidaio/tall-components",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Rub√©l",
            "email": "michael@laravel.software",
            "role": "Backend Engineer"
        },
        {
            "name": "Grzegorz Durtan",
            "email": "garygiovanismith@gmail.com",
            "role": "Frontend Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.4",
        "brianium/paratest": "^6.3",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.16",
        "pestphp/pest-plugin-global-assertions": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-parallel": "^0.2.0",
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Temidaio\\TallComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Temidaio\\TallComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Temidaio\\TallComponents\\TallComponentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
